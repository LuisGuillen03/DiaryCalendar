<div class="container p-4">
    <div class="row">
        <div class="container text-center my-auto">
            <h1 class="mb-5">
                <em>Calendarios</em>
            </h1>
        </div>
        {{#each calen}}
        <div class="col-md-6 mt-2">
            <div class="card text-center">
                <div class="card-body">
                    <a href="/calendar/{{Nom}}/months">
                    <img src="/img/{{Nom}}.png" alt="Logo Fazt" class="card-img-top mx-auto m-2 rounded-circle w-50">
                    </a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>